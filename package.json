  "license": "ISC",
  "devDependencies": {
    "cypress": "^6.8.0",
    "cypress-iframe": "^1.0.1",
    "cypress-sql-server": "^1.0.0",
    "mocha": "^8.3.2",
    "mochawesome": "^6.2.2"
  }
}
